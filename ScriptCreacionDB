/****** Object:  Database [UsersDb]    Script Date: 19/12/2020 21:33:37 ******/
CREATE DATABASE [UsersDb]
 CONTAINMENT = NONE
 ON  PRIMARY 
( NAME = N'UsersDb', FILENAME = N'C:\Users\lauti\UsersDb.mdf' , SIZE = 8192KB , MAXSIZE = UNLIMITED, FILEGROWTH = 65536KB )
 LOG ON 
( NAME = N'UsersDb_log', FILENAME = N'C:\Users\lauti\UsersDb_log.ldf' , SIZE = 8192KB , MAXSIZE = 2048GB , FILEGROWTH = 65536KB )
GO

IF (1 = FULLTEXTSERVICEPROPERTY('IsFullTextInstalled'))
begin
EXEC [UsersDb].[dbo].[sp_fulltext_database] @action = 'enable'
end
GO

